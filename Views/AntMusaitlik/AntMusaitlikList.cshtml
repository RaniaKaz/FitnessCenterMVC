@model IEnumerable<webProject.Models.AntMusaitlik>

@{
    ViewData["Title"] = "AntMusaitlikList";
}

<h1>AntMusaitlikList</h1>

<p>
    <a asp-action="AntMusaitlikOlustur">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Gun)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Baslangic)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Bitis)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Antrenor)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Gun)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Baslangic)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Bitis)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Antrenor.ID)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
